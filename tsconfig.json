{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "es5",
    "strict": true,
    "resolveJsonModule": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": ".",
    "paths": {
      "@router/*": ["router/*"],
      "@pages/*": ["pages/*"],
      "@icon/*": ["assets/icon/*"],

      "@api/*": ["common/api/*"],
      "@utils/*": ["common/utils/*"],
      "@styles/*": ["common/styles/*"],
      "@typings/*": ["common/typings/*"],
      "@hooks/*": ["common/hooks/*"],
      "@components/*": ["common/components/*"],
      "@atom/*": ["common/components/atom/*"],
      "@molecule/*": ["common/components/molecule/*"],
      "@organism/*": ["common/components/organism/*"],
      "@template/*": ["common/components/template/*"],
    }
  }
}
